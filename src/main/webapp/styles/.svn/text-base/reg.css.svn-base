@charset "utf-8";
@import url("uploadify.css");
@import url("jcrop.css");
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0;}table{border-collapse:collapse;border-spacing:0;}fieldset,img{border:0;}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}li{list-style:none;}caption,th{text-align:left;}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}q:before,q:after{content:'';}abbr,acronym{border:0;font-variant:normal;}sup{vertical-align:text-top;}sub{vertical-align:text-bottom;}input,textarea,select{font-family:inherit;font-size:inherit;font-weight:inherit;}input,textarea,select{*font-size:100%;}legend{color:#000;}
body{font:0.8em/150% verdana,simsung,arial,Tahoma,sans-serif;}a:link{font-weight:bold;text-decoration:none;color:#8F9E28;}a:visited{font-weight:bold; text-decoration:none;color:#C0C0C0;}a:hover, a:active{text-decoration:none;color:#5890D1;}
#container{background-color:#fff;padding-bottom:50px;}
#header{background:transparent url(../images/reg_bg.gif) repeat-x;height:73px;width:100%;}
#content{position:relative;margin-left:210px;min-width:960px;}
#left_side{float:left;width:210px;margin-left:-210px;margin-top:41px;background:#F1F0E5 url(../images/back_green-fade.gif) 0 0 repeat-x;min-height:450px;}
#left_side h1{position:relative;background: url(../images/tab_green.gif) 0 0 no-repeat;height:30px;width:166px;text-align:center;color:#fff;font:20px 'trebuchet ms';padding-top:2px;margin-top:-41px;margin-left:20px;line-height:29px;margin-bottom:8px;display:block;}
#proceede{height:200px;width:190px;top:15px;left:5px;padding-left:9px;padding-top:13px;background:url(../images/left_nav_callout.png) 0 0 no-repeat;position:relative;}
#proceede h2{font-size:1.2em;font-weight:bold;color:#333333;height:36px;padding-left:5px;}
#proceede a{background:url(../common/images/monitor24.gif) no-repeat 0px center;padding:5px 0px 5px 30px;display:block;color:#369;margin-bottom:5px;width:140px;}
#right_side{float:left;width:100%;margin-top:6px;}
#right_side ul.nav_title{position:relative;margin:10px 0;clear:both;}
#right_side table.reg_tb{text-align:left;margin:15px 0;clear:both;}
#right_side ul.nav_title li{list-style:none;float:left;}
#right_side h1{text-align:center;color:#5890D1;font-size:1.9em;font-family:Trebuchet MS;}
#right_side fieldset.fs_title{padding:10px;margin-top:5px;border-top:1px solid #666;border-left:1px solid #A2A09A;}
#right_side fieldset legend.leg_title{color:#000;font-weight:bold;padding:3px 20px 3px 20px;border:1px solid #666;background:#fff;}
#right_side dl{position:relative;width:756px;}
#right_side dd{font-size:1em;height:58px;width:376px;float:left;}
#right_side dd label{float:left;line-height:26px;}
#right_side dd label.lab{width:150px;text-align:right;display:block;color:#0E5080;}
#right_side dd input,#right_side dd span{float:left;height:20px;}
#right_side dd input.inp{width:200px;border:1px solid #628EB1;padding:2px;vertical-align:middle;}
#footer{clear:both;width:100%;margin:0 auto;text-align:center;}
#footer a:link, #footer a:visited{margin:0 10px;}
#footer ul{padding-bottom:15px;padding-top:10px;}
#footer li{display:inline;}
#crop_submit{margin-left:5px;margin-top:-5px;font-size:1em;font-weight:bold;color:#fff;cursor:pointer;}
#right_side dd input.aqua_but{background-image:url(../images/aqua_but.png);background-repeat:no-repeat;background-color:transparent;width:120px;height:36px;background-position:0 0;border-width:0;}
#right_side dd input.aqua_but:hover{background-position:0 -36px;color:#5890D1;}
#butSubmit{position:relative;}#userSubmit{width:140px;}div.buttonSubmit input,div.buttonSubmit span{margin:0 20px 70px 350px;height:36px;background-image:url(../images/button-submit.gif);background-repeat:no-repeat;background-position:right 0px;color:#FFF;font-weight:bold;padding:0px 15px 2px 15px;border:0px;cursor:pointer;z-index:5;}
div.buttonSubmit input{width:auto;}div.buttonSubmit span{width:4px;position:absolute;left:0px;top:0px;background-position:left -36px;padding:0px 0px 0px 0px;z-index:10;}body.safari div.buttonSubmit span{display:none}div.buttonSubmitHover input{background-position:right -72px;}div.buttonSubmitHover span{background-position:left -108px;}
#tmp_preview{width:80px;height:100px;overflow:hidden;}
#right_side dd label.error{display:block;color:red;font-style:italic;font-weight:normal;margin-left:150px;}#right_side dd input[type="text"].error, #right_side dd select.error{width:200px;}#right_side dd input.error, #right_side dd select.error{border:2px solid red;background-color:#FFFFD5;margin:0px;color:red;}div.error{color:red;}div.error a{color:#336699;font-size:1em;text-decoration:underline}
#proceede a.step{font-weight:normal;text-decoration:none;cursor:default;}
#proceede a.step1{color:#360;background-image:url(../images/step1-24.gif);}#proceede a.step2{background-image:url(../images/step2-24.gif);}#proceede a.step3{background-image:url(../images/step3-24.gif);}
#right_side dl.addr{background:url(../images/divider.gif) left 17px repeat-x;}
#right_side dl legend{color:#693;font-weight:bold;}
#isame{float:left;display:inline;width:auto;margin-left:700px;}
#right_side table caption{text-align:center;}
#right_side table{width:650px;margin:0px auto; margin-bottom:30px;color:#333;text-align:center;border-collapse:collapse;}
#right_side table td,#right_side table th{border:1px solid #999;height:22px;text-align:center;}
#right_side table th{font-weight:bold;background:#c5c5c5;color:#333;line-height:28px;height:28px;}
.t_even{background-color:#eee;}.t_over{background-color:#FEF3D1;}
.tmp_view{position:relative;}
#sfz,#touxian{float:left;margin:30px 5px;}
#thumbnail{clear:both;position:relative;max-width:670px;}
.idScan_preview{width:450px;height:270px;overflow:hidden;}
.portrait_preview{width:80px;height:100px;overflow:hidden;}